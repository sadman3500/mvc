{
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    
    "require": {
        "symfony/http-foundation": "^3.3",
        "symfony/http-kernel": "^3.3",
        "hassankhan/config": "^0.10.0",
        "symfony/dotenv": "^3.3",
        "monolog/monolog": "^1.22",
        "symfony/routing": "^3.3"
    },
    
   
   
    
    "autoload": {
        "psr-4": {
          "Core\\":"src/classes",
          "Test\\":"src/tests"
        }
    },
    
